   "source": [
    "# Number of time weather is clear\n",
    "data[\"Weather\"].value_counts()      # Method 1(valuecount)\n",
    "data[data.Weather==\"Clear\"]         #Method 2 (filtering)\n",
    "data.groupby(\"Weather\").get_group(\"Clear\")         # Methopd 3 (groubby)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
